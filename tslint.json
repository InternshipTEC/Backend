{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-airbnb", "tslint-config-prettier", "tslint-plugin-prettier"],
  "jsRules": {},
  "rules": {
    "import-name": false,
    "variable-name":[
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ],
    "no-console": false,
    "object-literal-sort-keys": false,
    "prettier": true,
    "trailing-comma": [false],
    "type-literal-delimiter": false
  },
  "rulesDirectory": []
}
